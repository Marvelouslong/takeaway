<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app metadata-complete="false" version="3.1"
        xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://xmlns.jcp.org/xml/ns/javaee">

  <display-name>Archetype Created Web Application</display-name>
  <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
  <welcome-file-list>
    <welcome-file>loginadmin.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>loginadminServlet</servlet-name>
    <servlet-class>controller.loginadminServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>loginadminServlet</servlet-name>
    <url-pattern>/loginadmin.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>loginstoreServlet</servlet-name>
    <servlet-class>controller.loginstoreServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>loginstoreServlet</servlet-name>
    <url-pattern>/loginstore.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>userloginServlet</servlet-name>
    <servlet-class>controller.userloginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>userloginServlet</servlet-name>
    <url-pattern>/loginuser.do</url-pattern>
  </servlet-mapping>
</web-app>
